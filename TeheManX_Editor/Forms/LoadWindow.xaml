<Window x:Class="TeheManX_Editor.Forms.LoadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeheManX_Editor.Forms"
        mc:Ignorable="d"
        Title="Load Data" Height="102" Width="199" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closed="Window_Closed">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Button Click="palBtn_Click">Load Palette</Button>
        <local:NumInt Grid.Column="1" FontSize="18" Minimum="0" Maximum="#FFFF" Increment="2" x:Name="paletteInt" Width="85" Value="#0"></local:NumInt>

        <Button Click="tileBtn_Click" Grid.Row="1">Load Tiles</Button>
        <local:NumInt Grid.Row="1" Grid.Column="1" FontSize="18" Minimum="0" Maximum="#FF" x:Name="tilesInt" Width="85" Value="#0"></local:NumInt>
    </Grid>
</Window>
